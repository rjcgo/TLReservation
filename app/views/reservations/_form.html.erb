<%= form_for([@testline,@testline.reservations.build]) do |f| %>
    <p> <%= f.label :name,'Title: '%>
        <%= f.text_field :name %>
    </p>
    <p> <%= f.label :team_name,'Team Name/Username: '%>
        <%= f.text_field :team_name %>
    </p>
    <p> <%= f.label :description,'Description: '%>
        <%= f.text_area :description %>
    </p>
    <p>
        <%= f.submit %>
    </p>
        
<% end %>