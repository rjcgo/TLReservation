<% if user_signed_in? %>
	<% if current_user.isAdmin? %>
		<main class="wrapper">
		    <section id="add-testline">
				<h2>Edit TestLine</h2>
				<%= render 'form' %>
			</section>
		</main>
	<% else %>
		<%= render partial: "admin/error" %>
	<% end %>
<% else %>
	<%= redirect_to "/login" %>
<% end %>