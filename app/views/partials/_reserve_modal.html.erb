<div id="reserve-modal" class="modal">
	<div class="modal-dialog">
        <div class="modal-content">
            <%= form_for("/testlines/12") do %>
                <div class="modal-header">
                    <button type="button" class="modal-close" data-dismiss="modal">&times;</button>
                    <h3 class="modal-title"></h3>
                </div>
                <div class="modal-body">
                    <label for="title">Title:</label>
                    <input type="text" placeholder="Test title" class="form-control" id="title" required>

                    <label for="team-name">Team Name:</label>
                    <input type="text" value="" class="form-control" id="team-name" readonly>

                    <label for="email">Email:</label>
                    <input type="email" value="" class="form-control" id="email" readonly>
                    
                    <label for="description">Description:</label>
                    <textarea placeholder="Test description" class="form-control" id="description" required>
                    </textarea>
                </div>
                <div class="modal-footer">
                    <button type="submit" class="modal-btn" data-dismiss="modal">Submit</button>
                </div>
            <% end %>
        </div>
    </div>
</div>