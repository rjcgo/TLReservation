<% if user_signed_in? && current_user.isAdmin? %>
    <main class="wrapper" id="sidenav-container">
        <%= render partial: 'sidenav', :locals => {:sect => "associations"} %>
            <section id="associations-admin">
                <header>
                    <%= render partial: 'header', :locals => header("Associations") %>
                    <%= admin_notice(@testlines, "associations", notice) %>
                </header>
                <div class="card-deck">
                    <% @testlines.each do |testline| %>
                        <%= render partial: "partials/card", locals: cardProps(testline) %>
                    <% end %>
                </div>
            </section>
    </main>
    <%= render partial: "partials/img_modal" %>
    <%= render partial: "partials/description_modal" %>
    <%= render partial: "partials/rel_modal" %>
    <%= render partial: "partials/confirm_modal", :locals => modal_content("teams") %>
<% else %>
    <%= render 'error' %>
<% end %>